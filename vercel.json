{
  "version": 2,
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/node@1.11.0"
    }
  ],
  "functions": {
    "api/**/*.js": {
      "runtime": "@vercel/node",
      "memory": 1024, // Опционально: установите размер памяти
      "timeout": 30 // Установите время ожидания в секундах
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NODE_VERSION": "18.13.0"
  }
}
